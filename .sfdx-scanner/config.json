{
	"engines": [
		{
			"name": "eslint",
			"targetPatterns": ["**/*.js", "!**/lwc/**/*.js", "!**/node_modules/**", "!**/bower_components/**", "!**/jquery*.js"],
			"disabled": false
		},
		{
			"name": "eslint-lwc",
			"targetPatterns": ["**/lwc/**/*.js", "!**/node_modules/**"],
			"disabled": true
		},
		{
			"name": "CPD",
			"targetPatterns": ["**/*.cls", "**/*.trigger", "**/*.java", "**/*.page", "**/*.component", "**/*.xml", "!**/node_modules/**", "!**/*-meta.xml"],
			"disabled": true,
			"minimumTokens": "100"
		}
	]
}
